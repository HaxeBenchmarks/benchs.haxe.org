Header set Strict-Transport-Security "max-age=15768000" env=HTTPS
Header set X-Frame-Options: SAMEORIGIN
Header set X-Content-Type-Options: nosniff
Header set X-XSS-Protection "1; mode=block"
Header set Content-Security-Policy "default-src 'none'; script-src 'self'; connect-src 'self'; base-uri 'none'; style-src 'self' 'unsafe-inline'; font-src 'self'; object-src 'none'; img-src 'self' https://haxe.org/; form-action 'self'; "
Header set Referrer-Policy "no-referrer"
